{
  "name": "carbon-tutorial-nextjs",
  "version": "0.1.0",
  "private": true,
  "repository": "git@github.com:carbon-design-system/carbon-tutorial-nextjs.git",
  "bugs": "https://github.com/carbon-design-system/carbon-tutorial-nextjs/issues",
  "license": "Apache-2.0",
  "scripts": {
    "build": "next build",
    "dev": "cross-env RUNTIME_IDC_NAME=sg next dev",
    "start": "next start",
    "lint": "next lint",
    "ci-check": "yarn format:diff",
    "clean": "yarn cache clean && yarn install",
    "format": "prettier --write \"**/*.{js,md,scss}\"",
    "format:diff": "prettier --list-different \"**/*.{js,md,scss}\""
  },
  "dependencies": {
    "@carbon/charts-react": "^1.15.5",
    "@carbon/icons-react": "^11.36.0",
    "@carbon/react": "^1.33.0",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "axios": "^1.6.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.373.0",
    "moment": "^2.30.1",
    "next": "13.4.9",
    "papaparse": "^5.4.1",
    "radix-ui": "^1.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-unity-webgl": "^9.5.1",
    "recharts": "^2.12.7",
    "sass": "^1.63.6",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "autoprefixer": "^10.4.17",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "postcss": "^8.4.35",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.4.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --cache --write",
      "git add"
    ],
    "*.{scss,css}": [
      "prettier --cache --write",
      "git add"
    ],
    "*.md": [
      "prettier --cache --write",
      "git add"
    ]
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
